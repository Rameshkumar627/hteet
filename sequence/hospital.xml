<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">

        <!-- Product : arc.product -->
        <record id="arc_product_sequence" model="ir.sequence">
            <field name="name">Product</field>
            <field name="code">arc.product</field>
            <field name="prefix">PRT</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Product : arc.pharmacy -->
        <record id="arc_pharmacy_sequence" model="ir.sequence">
            <field name="name">Pharmacy</field>
            <field name="code">arc.pharmacy</field>
            <field name="prefix">PHR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Product : stock.move -->
        <record id="arc_move_sequence" model="ir.sequence">
            <field name="name">Stock Move</field>
            <field name="code">arc.move</field>
            <field name="prefix">MV</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Product : stock.move -->
        <record id="stock_inward_sequence" model="ir.sequence">
            <field name="name">Stock Inward</field>
            <field name="code">stock.transact.in</field>
            <field name="prefix">SI</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Product : stock.transact.scrap -->
        <record id="stock_scrap_sequence" model="ir.sequence">
            <field name="name">Stock Scrap</field>
            <field name="code">stock.transact.scrap</field>
            <field name="prefix">SI</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Product : stock.transact.block -->
        <record id="stock_block_sequence" model="ir.sequence">
            <field name="name">Stock Block</field>
            <field name="code">stock.transact.block</field>
            <field name="prefix">SI</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

    </data>

</odoo>
